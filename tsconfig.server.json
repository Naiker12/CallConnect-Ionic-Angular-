{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2020",
      "outDir": "./dist-server",
      "types": ["node"],
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src/server/**/*.ts"],
    "exclude": ["src/app/**/*", "**/*.spec.ts"]
  }